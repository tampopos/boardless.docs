{
  "files.exclude": {
    "**/.data": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "**/dist": true,
    "**/bin": true,
    "**/obj": true
  },
  "files.autoSave": "onFocusChange",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.detectIndentation": false,
  "editor.tabCompletion": true,
  "prettier.semi": true,
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.disableLanguages": ["jchannon.csharpextensions"],
  "cSpell.words": [
    "Autofac",
    "CQRS",
    "Hmac",
    "IBoardless",
    "ISQS",
    "Includable",
    "Npgsql",
    "Protobuf",
    "SQSDI",
    "Xunit",
    "appsettings",
    "boardless",
    "classname",
    "gitignore",
    "hoge",
    "huga",
    "msgs",
    "nuget",
    "postgres",
    "postgresql",
    "psql",
    "tmpps"
  ],
  "cSpell.ignoreRegExpList": ["/.*ãƒ¼/"],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/appsettings.*.json",
    ".vscode",
    "typings"
  ]
}
